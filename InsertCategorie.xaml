<Window x:Class="MagazinElectronic.InsertCategorie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MagazinElectronic"
        mc:Ignorable="d"
        Title="InsertCategorie" Height="300" Width="465">
    <Grid>
        <TextBox x:Name="Tb_Categorie" HorizontalAlignment="Center" VerticalAlignment="Center" Width="201" Height="60" FontFamily="Arial Black" FontSize="20" TextAlignment="Center" TextWrapping="Wrap" TextChanged="Tb_Categorie_TextChanged"/>
        <Button x:Name="Insert_button" Content="INSERT" HorizontalAlignment="Left" Margin="343,198,0,0" VerticalAlignment="Top" Width="80" Height="58" Foreground="Black" FontFamily="Italic" FontSize="14" Background="#FFC2C2C2" Click="Insert_button_Click"/>

    </Grid>
</Window>
